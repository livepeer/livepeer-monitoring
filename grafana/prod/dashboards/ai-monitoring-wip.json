        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "cdrta4zbtlddsa"
          },
          "editorType": "sql",
          "format": 1,
          "meta": {
            "builderOptions": {
              "columns": [],
              "database": "",
              "limit": 1000,
              "mode": "list",
              "queryType": "table",
              "table": ""
            }
          },
          "pluginVersion": "4.7.0",
          "queryType": "table",
          "rawSql": "SELECT \n    se.stream_id,\n    se.type,\n    se.sub_type,\n    se.gateway_address,\n    se.gateway_host,\n    se.orchestrator_address,\n    se.orchestrator_url,\n    fromUnixTimestamp(CAST(se.timestamp_ms / 1000 AS Int64)) AS Time_Stamp, \n    se.restart_count,\n    ss.state,\n    se.last_error\nFROM \n    network_events.stream_events AS se\nJOIN \n    network_events.stream_status AS ss ON se.stream_id = ss.stream_id\nWHERE \n    (se.gateway_address IS NOT NULL AND se.gateway_address != '')\n    OR (se.orchestrator_address IS NOT NULL AND se.orchestrator_address != '')\n    AND (se.stream_id IS NOT NULL AND se.stream_id != '')\nORDER BY \n    Time_Stamp DESC,\n    se.stream_id ASC\nLIMIT 100;\n",
          "refId": "A"
        }
      ],
      "title": "Stream Error Logs",
      "type": "table"
    },
